<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="178.0" prefWidth="600.0" styleClass="dialog-content"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="GUI.Chat.AddConversationDialogContent">
   <stylesheets>
      <URL value="@../../styles/CSS/style.css"/>
      <URL value="@../../styles/CSS/customStyle.css"/>
   </stylesheets>
   <children>
      <BorderPane layoutX="176.0" layoutY="100.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <Label styleClass="chat-title" text="Create new conversation" BorderPane.alignment="CENTER"/>
         </top>
         <center>
            <VBox prefHeight="200.0" prefWidth="600.0" spacing="15.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="recipientIdField" promptText="Recipient ID:"/>
                  <TextField fx:id="conversationNameField" promptText="Conversation Name:"/>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
