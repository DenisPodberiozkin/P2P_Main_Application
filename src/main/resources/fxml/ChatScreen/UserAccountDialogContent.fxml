<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="220.0" prefWidth="538.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="GUI.Chat.UserAccountDialogContent">
   <children>
      <BorderPane prefHeight="220.0" prefWidth="685.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <AnchorPane styleClass="dialog-content">
               <children>
                  <VBox layoutX="15.0" layoutY="15.0" prefHeight="188.0" prefWidth="685.0" spacing="15.0"
                        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                        AnchorPane.topAnchor="0.0">
                     <children>
                        <Label styleClass="information-label" text="User ID:"/>
                        <TextField fx:id="idField" editable="false" prefHeight="44.0" prefWidth="618.0"/>
                        <GridPane>
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="394.0" minWidth="10.0" prefWidth="254.0"/>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="588.0" minWidth="10.0" prefWidth="284.0"/>
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                           </rowConstraints>
                           <children>
                              <Label prefHeight="27.0" text="User IP:Port"/>
                              <FlowPane prefHeight="44.0" prefWidth="327.0" GridPane.columnIndex="1">
                                 <children>
                                    <TextField fx:id="applicationIpField" editable="false" prefHeight="44.0"
                                               prefWidth="187.0" styleClass="ip-field"/>
                                    <Label text=":"/>
                                    <TextField fx:id="applicationPortField" editable="false" prefHeight="44.0"
                                               prefWidth="97.0" styleClass="port-field"/>
                                 </children>
                              </FlowPane>
                           </children>
                        </GridPane>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </center>
         <top>
            <FlowPane BorderPane.alignment="CENTER">
               <children>
                  <Label styleClass="chat-title" text="User Account - "/>
                  <Label fx:id="usernameLabel" styleClass="chat-title" text="Username"/>
               </children>
            </FlowPane>
         </top>
         <stylesheets>
            <URL value="@../../styles/CSS/style.css"/>
            <URL value="@../../styles/CSS/customStyle.css"/>
         </stylesheets>
      </BorderPane>
   </children>
</AnchorPane>
